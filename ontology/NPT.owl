    <!-- http://purl.obolibrary.org/obo/NPT_0012073 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/NPT_0012073">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/NPT_0012028"/>
        <obo:IAO_0000114 rdf:resource="http://purl.obolibrary.org/obo/IAO_0000123"/>
        <obo:IAO_0000115>A linguistic cognitive function which, when realized, is the means through which words or sounds are mimicked.</obo:IAO_0000115>
        <obo:IAO_0000117>Alexander P. Cox</obo:IAO_0000117>
        <rdfs:label>language repetition cognitive function</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/NPT_0012074 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/NPT_0012074">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/NPT_0012028"/>
