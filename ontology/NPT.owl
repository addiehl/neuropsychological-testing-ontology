    <!-- http://purl.obolibrary.org/obo/NPT_0012076 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/NPT_0012076">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/NPT_0012028"/>
