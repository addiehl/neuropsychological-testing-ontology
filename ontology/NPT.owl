        <rdfs:label>speech production cognitive function</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/NPT_0012075 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/NPT_0012075">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/NPT_0012028"/>
